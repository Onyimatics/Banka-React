{
  "name": "banka-react",
  "version": "1.0.0",
  "description": "Banka is a light-weight core banking application that powers banking operations like account creation, customer deposit and withdrawals. This app is meant to support a single bank, where users can signup and create bank accounts online, but must visit the branch to withdraw or deposit money.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint **/*.js",
    "lint:fix": "prettier-eslint \"**/*.{js,json}\" --write",
    "prettier": "prettier **/*.{js,json} --write",
    "develop": "webpack --env.mode development --watch",
    "start": "node server.js",
    "start:dev": "rm -rf client/dist && webpack-dev-server --env.mode development --open --hot",
    "test": "jest --coverage",
    "heroku-postbuild": "npm run build",
    "build": "webpack --env.mode production",
    "test:watch": "jest --watch --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Onyimatics/Banka-React.git"
  },
  "author": "Onyimatics",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Onyimatics/Banka-React/issues"
  },
  "homepage": "https://github.com/Onyimatics/Banka-React#readme",
